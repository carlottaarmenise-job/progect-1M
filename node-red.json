            ]
        ]
    },
    {
        "id": "58b48cc59be22b54",
        "type": "function",
        "z": "6560bab65fc2abf1",
        "name": "Cancel order logic",
        "func": "// Annulla un ordine\nlet orders = global.get(\"orders\") || [];\nlet orderId = parseInt(msg.req.params.id);\n\n// Trova l'ordine\nlet orderIndex = orders.findIndex(o => o.id === orderId);\n\nif (orderIndex === -1) {\n    msg.statusCode = 404;\n    msg.payload = { error: \"Ordine non trovato\" };\n    return msg;\n}\n\n// Controlla se l'ordine pu√≤ essere annullato\nif (orders[orderIndex].status === \"delivered\" || orders[orderIndex].status === \"cancelled\") {\n    msg.statusCode = 400;\n    msg.payload = { error: \"Impossibile annullare questo ordine\" };\n    return msg;\n}\n\n// Annulla l'ordine\norders[orderIndex].status = \"cancelled\";\norders[orderIndex].cancelledDate = new Date().toISOString();\n\nglobal.set(\"orders\", orders);\n\nmsg.payload = orders[orderIndex];\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
