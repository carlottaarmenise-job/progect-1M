    {
        "id": "eae36a1fee8534cf",
        "type": "function",
        "z": "6560bab65fc2abf1",
        "name": "Create order logic",
        "func": "// Crea un nuovo ordine\nlet orders = global.get(\"orders\") || [];\nlet order = msg.payload;\n\n// Genera ID e timestamp\norder.id = Date.now();\norder.orderDate = new Date().toISOString();\norder.status = \"pending\";\n\n// Genera tracking number\norder.trackingNumber = \"TRK\" + Math.random().toString(36).substr(2, 9).toUpperCase();\n\n// Salva l'ordine\norders.push(order);\nglobal.set(\"orders\", orders);\n\n// Restituisce l'ordine creato\nmsg.payload = order;\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 730,
        "y": 2180,
        "wires": [
            [
                "d1efb8f792aa39b8"
            ]
        ]
    },
    {
        "id": "6bed0b62b97b5664",
        "type": "function",
        "z": "6560bab65fc2abf1",
        "name": "Update order logic",
        "func": "// Aggiorna un ordine esistente\nlet orders = global.get(\"orders\") || [];\nlet orderId = parseInt(msg.req.params.id);\nlet updateData = msg.payload;\n\n// Trova l'ordine\nlet orderIndex = orders.findIndex(o => o.id === orderId);\n\nif (orderIndex === -1) {\n    msg.statusCode = 404;\n    msg.payload = { error: \"Ordine non trovato\" };\n    return msg;\n}\n\n// Aggiorna l'ordine\norders[orderIndex] = { ...orders[orderIndex], ...updateData };\n\n// Aggiorna timestamp\norders[orderIndex].updatedAt = new Date().toISOString();\n\nglobal.set(\"orders\", orders);\n\nmsg.payload = orders[orderIndex];\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 730,
        "y": 2260,
